paths:
{{#each paths as |path i|}}
  - {{path}}
{{/each}}
exclude_files: ['\.gz$']
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag i|}}
  - {{tag}}
{{/each}}
{{#if tz_offset}}
fields_under_root: true
fields:
  _conf:
    tz_offset: {{tz_offset}}
{{/if}}
{{#if processors}}
{{processors}}
{{/if}}